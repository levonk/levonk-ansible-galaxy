# ActivityWatch Configuration
# Generated by Ansible quantified-self role

[server]
host = "127.0.0.1"
port = 5600

[client]
commit_interval = 10

[watchers]
# Enable/disable watchers
aw-watcher-afk = true
aw-watcher-window = true
aw-watcher-web = true

[privacy]
# Privacy settings
retain_data_days = 365
anonymize_data = false